{
  "name": "nebula-api-client",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@1.3.8",
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.1.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "preview": "next build && next start",
    "lint": "biome check ./src",
    "lint:fix": "biome check --write ./src",
    "format": "biome format --write ./src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "knip": "knip",
    "clean": "rm -rf .next node_modules/.cache src/.next",
    "clean:all": "rm -rf .next node_modules src/.next bun.lock",
    "validate": "bun run type-check && bun run lint && bun run test",
    "ci": "bun run type-check && bun run lint && bun run build && bun run test",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "@hugeicons/core-free-icons": "^3.1.1",
    "@hugeicons/react": "^1.1.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.33.0",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.7.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.3",
    "jsdom": "^28.0.0",
    "knip": "^5.83.1",
    "lefthook": "^2.1.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
